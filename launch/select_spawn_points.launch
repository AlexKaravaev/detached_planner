<launch>
  <master auto="start" />
  <param name="/use_sim_time" value="true" />
	  <node pkg="stage_ros" type="stageros" name="stageros" args="$(find detached_planner)/stage/maze_clear.world">
     <param name="base_watchdog_timeout" value="0.2"/>
  </node>

  <node name="map_server" pkg="map_server" type="map_server" args="$(find detached_planner)/maps/maze.yaml" output="screen">
	<param name="frame_id" value="map" />
  </node>
  <node pkg="detached_planner" name="robosim" type="robot_sim.py" output="screen" /> 

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find detached_planner)/cfg/spawn.rviz" />

</launch>
